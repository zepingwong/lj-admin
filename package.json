{
  "name": "lj-vue3-admin-monorepo",
  "version": "0.1.7",
  "private": "true",
  "description": "Datav for vue3+vite",
  "homepage": "https://github.com/vaemusic/datav-vue3",
  "bugs": {
    "url": "https://github.com/vaemusic/datav-vue3/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaemusic/datav-vue3.git"
  },
  "license": "MIT",
  "directories": {
    "doc": "docs",
    "test": "test",
    "lint:lint-staged": "lint-staged"
  },
  "scripts": {
    "build:admin": "pnpm run --filter @lj/admin build",
    "build:docs": "pnpm run build:lib && pnpm run build",
    "dev:admin": "pnpm run --filter @lj/admin dev",
    "dev:docs": "pnpm run --filter @lj/docs dev",
    "gen": "node ./script/genNewComp/index.js",
    "lint": "eslint .",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "~5.47.1",
    "@typescript-eslint/parser": "~5.47.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "commitizen": "^4.2.6",
    "eslint": "^8.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "fs-extra": "^11.1.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.3",
    "inquirer": "^9.1.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.1.0",
    "pnpm": "^6.32.24",
    "prettier": "^2.8.2",
    "sort-package-json": "^2.1.0",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.12.1",
    "vite": "^4.0.0",
    "vitest": "^0.26.3",
    "vue-tsc": "^1.0.22"
  },
  "packageManager": "pnpm@6.32.3"
}
